{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "cyto-snap",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "cli": {
      "description": "Render graphs on the server side with Cytoscape.js, getting image files as output",
      "longDescription": "This package is similar to cytosnap, but is uses Tauri instead of Puppeteer",
      "beforeHelp": "",
      "afterHelp": "",
      "args": [
        { "name": "source", "short": "s", "takesValue": true },
        { "name": "destination", "short": "d", "takesValue": true }
      ],
      "subcommands": {}
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.stereobooster.cytosnap",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "visible": false,
        "fullscreen": false,
        "resizable": true,
        "title": "cyto-snap",
        "width": 800,
        "height": 600
      }
    ]
  }
}
